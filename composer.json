{
    "name": "figuren-theater/ft-options",
    "description": "Options Management for a WordPress Multisite like figuren.theater",
    "type": "wordpress-muplugin",
    "license": "GPL-3",
    "version": "1.1.3",
    "autoload": {
        "psr-4": {
            "Figuren_Theater\\Options\\": "src/"
        }
    },
    "authors": [
        {
            "name": "figuren.theater",
            "email": "info@figuren.theater"
        }
    ],
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/figuren-theater/ft-core"
        },
        {
            "type": "composer",
            "url": "https://packagist.org"
        }
    ],
    "require": {
        "php": ">=7.1",
        "figuren-theater/ft-core": "dev-main",
        "composer/installers": "~1"
    }
}
